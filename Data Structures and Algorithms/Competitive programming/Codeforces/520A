#include<bits/stdc++.h>
using namespace std;

void solve(){
	int n;
	cin >> n;
	string s;
	cin >> s;

	string temp = s;

	for(int i = 0; i<temp.size(); i++){
		if(temp[i] >= 'A' && temp[i] <= 'Z'){
			temp[i] += 32;
		}
	}
    
    sort(temp.begin(), temp.end());

    string ans = "";

    for(int i = 0; i<n; i++){
    	if(temp[i] != temp[i+1]){
            ans += temp[i];
    	}
    }

    if(ans.size() == 26){
    	cout << "YES";
    }
    else{
    	cout << "NO";
    }
}

int main(){
	int t = 1;
	while(t--){
		solve();
	}
}
