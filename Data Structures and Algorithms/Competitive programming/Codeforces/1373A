#include<bits/stdc++.h>
using namespace std;

#define endl '\n'
#define ll long long

void solve(){
	ll a, b, c;
	cin >> a >> b >> c;
	if(a < c){
             cout << 1 << " ";
	}
	else{ 
	     cout << -1 << " ";
	}
	
	if(b * a > c){
	     cout << b << " ";
	}
	else{
	     cout << -1 << " ";
	}
       cout << endl;
}

int main(){
	ll t;
	cin >> t;
	while(t--){
	     solve();
	}
}
