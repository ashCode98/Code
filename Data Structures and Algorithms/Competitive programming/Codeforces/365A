#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define endl '\n'

void solve(){
    ll n, k;
    cin >> n >> k;

    ll cnt = 0;
    for(ll i = 0; i<n; i++){
        string s;
        cin >> s;

        ll ansCnt = 0;
        vector<int>ans(15);

        for(ll i = 0; i<(int)s.size(); i++){
            ans[s[i]-'0']++;
        }

        bool ok = true;
        for(ll i = 0; i<=k; i++){
            if(ans[i] == 0){
                ok = false;
            }
        }

        cnt += ok;
    }

    cout << cnt;

}

int main(){
    
    ll t = 1;
    while(t--){
        solve();
    }
        
}
