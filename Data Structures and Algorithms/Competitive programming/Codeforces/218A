#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define endl '\n'
int n, k;

void solve(){

    cin >> n >> k;
    n = n + n + 1;
    
    vector<int> v(n);

    for(int i = 0; i<n; i++){
        cin >> v[i];
    }

    for(int i = 1; i<n; i+=2){
            if(v[i-1] < v[i]-1 && v[i]-1 > v[i+1]){
                v[i] = v[i] - 1;
                k = k - 1;
            }

        if(k == 0) break;
    }

    for(int i = 0; i<n; i++) {cout << v[i] << " ";}
}

int main(){
#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
#endif

    int t = 1;
    // cin >> t;
    while(t--){
    	solve();
    }
    
}
