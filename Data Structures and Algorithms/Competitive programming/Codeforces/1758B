#include<bits/stdc++.h>
using namespace std;

void solve(){
	int n;
	cin >> n;

	if(n % 2 != 0){
		while(n--){
		    cout << 3 << " ";
		}
		cout << '\n';
	}
	else if(n % 2 == 0){
		cout << 1 << " ";
		cout << 3 << " ";
		for(int i = 0; i<n-2; i++){
			cout << 2 << " ";
		}
		cout << '\n';
	}
}

int main(){

    int t;
    cin >> t;
    while(t--){
    	solve();
    }
}
