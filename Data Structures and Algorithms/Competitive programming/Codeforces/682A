#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define endl '\n'

void solve(){
	ll n, m;
	cin >> n >> m;

    vector<ll> a(5, 0), b(5, 0);

    for(ll i = 1; i<=n; i++){
    	a[i%5]++;
    }

    for(ll i = 1; i<=m; i++){
    	b[i%5]++;
    }

    cout << a[0]*b[0] + a[1]*b[4] + a[2]*b[3] + a[3]*b[2] + a[4]*b[1];

}

int main(){
#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
#endif

    int t = 1;
    // cin >> t;
    while(t--){
    	solve();
    }
    
}
